+static inline void ag7240_phy_setup(int unit)
+{
+#ifdef CONFIG_AR7242_S16_PHY
+    if ((is_ar7242() || is_wasp()) && (unit==0)) {
+        athrs16_phy_setup(unit);
+    } else
+#endif
+#ifdef CONFIG_ATHRS17_PHY
+    if (unit == 0) {
