+#define AG7100_RX_STATUS_PKT_RCVD       (1 << 0)
+#define AG7100_RX_STATUS_OVF            (1 << 2)
+#define AG7100_RX_STATUS_BUS_ERROR      (1 << 3)
+
+/*
+ * Int and int mask
