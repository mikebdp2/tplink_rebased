+        udelay(1000);
+
+        ag7240_reg_wr(mac, AG7240_MAC_MII_MGMT_CFG, mgmt_cfg_val | (1 << 31));
+        ag7240_reg_wr(mac, AG7240_MAC_MII_MGMT_CFG, mgmt_cfg_val);
+
+        return;
+    }
+#endif
+
+#ifdef CFG_ATHRS27_PHY
+    if (is_wasp()) {
+        printf("WASP ----> S27 PHY \n");
